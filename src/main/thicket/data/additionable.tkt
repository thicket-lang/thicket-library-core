module Data.Additionable

from Data.Number import number

model Additionable[a] {
    unbox : a
    (-)   : number -> a    
    (+)   : number -> a    
}

trait additionable[a] {
    with Additionable[a]
} {
    def (-) a = self + (a negate)
}
