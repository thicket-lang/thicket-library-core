module Data.Pair 
 
// Note: syntax simplification (a,b) === Pair[a,b]

model Pair[a,b] {
   _1 : a
   _2 : b
}

class pair[a,b] this:Pair[a,b] {
   unbox : Pair[a,b]
   map2 : [c,d] (a -> c) -> (b -> d) -> pair[c,d]
   
} {
   def unbox = this
   def map2 a b = pair $ Pair (a this._1) (b this._2)
}
