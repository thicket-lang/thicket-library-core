module Standard.Math

from Data.Number import number

model Math {
    max : number -> number -> number
    min : number -> number -> number
}

def math : Math = {
    Math 
        /*max*/ (n m -> n ?> m fold n m)
        /*min*/ (n m -> n ?> m fold m n)
}
