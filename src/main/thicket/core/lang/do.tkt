module Lang.Do

/* 
 * Language syntactic extension
 * 
 *     do A1 ; A2 ; A3 return A4
 */

class do[a] this:a {
    (;) : [b] b -> do[b]
    return : [c] c -> c
} {
    def (;) = do
    def return c = c
}
